{
  "name": "picturesvista-lib",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
